<h2>Sign up</h2>


<%= bootstrap_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <%= f.text_field :first_name, autofocus: true, label: "First Name" %>
  <%= f.text_field :last_name, label: "Last Name" %>
  <%= f.email_field :email %>
  <%= f.password_field :password, autocomplete: "off" %>
  <%= f.password_field :password_confirmation, autocomplete: "off", label: "Password Confirmation" %>

  <%= f.submit "Sign Up" %>
<% end %>

<br>
Or <%= render "devise/shared/links" %>
